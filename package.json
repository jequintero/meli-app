{
  "name": "meli-app",
  "version": "1.0.0",
  "description": "meli technical challenge",
  "main": "index.js",
  "repository": "https://github.com/jequintero/meli-app.git",
  "author": "Eduardo Quintero",
  "license": "MIT",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json,css}\"",
    "test": "node scripts/test.js",
    "start:prod": "node scripts/startProd.js",
    "build": "node scripts/build.js",
    "docker:build": "docker build --rm -t jequintero/meli-app .",
    "docker:start": "docker run --rm -it -p 3000:3000 jequintero/meli-app",
    "docker": "yarn run docker:build && yarn run docker:start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn run lint"
    ],
    "*.{js,json,css}": [
      "yarn run format",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-formatjs": "^10.3.18",
    "eslint": "^6.3.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^4.0.0",
    "prettier": "^1.18.2",
    "react-dev-utils": "^12.0.0",
    "react-test-renderer": "^17.0.2",
    "supertest": "^6.2.2",
    "webpack-dev-middleware": "^3.7.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.2",
    "compression": "^1.7.4",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "helmet": "^3.20.1",
    "import-glob-loader": "^1.1.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.8.0",
    "morgan": "^1.9.1",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
